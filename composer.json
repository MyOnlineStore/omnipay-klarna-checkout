{
    "name": "myonlinestore/omnipay-klarna-checkout",
    "type": "library",
    "description": "Klarna Checkout gateway for Omnipay payment processing library",
    "keywords": ["Klarna Checkout", "Omnipay"],
    "homepage": "https://github.com/MyOnlineStore/omnipay-klarna-checkout",
    "license": "MIT",
    "autoload": {
        "psr-4" : {
            "MyOnlineStore\\Omnipay\\KlarnaCheckout\\" : "src/",
            "MyOnlineStore\\Tests\\Omnipay\\KlarnaCheckout\\" : "tests/"
        }
    },
    "require": {
        "php": "^7.0",
        "omnipay/common": "~3",
        "ext-json": "*"
    },
    "require-dev": {
        "omnipay/tests": "~3"
    },
    "config": {
        "vendor-dir": "vendor",
        "sort-packages": true
    }
}
